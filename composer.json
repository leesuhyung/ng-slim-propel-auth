{
  "name": "previewict/pst-stack",
  "description": "Propel-Slim-Twig PHP Application Stack. The most sophisticated application stack for starting any PHP application in a minute.",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "propel",
    "slim",
    "twig",
    "stack",
    "sekeleton"
  ],
  "homepage": "https://github.com/previewict/pst-stack",
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "monolog/monolog": "1.*",
    "slim/slim": "^3.0",
    "propel/propel": "~2.0@dev",
    "ircmaxell/password-compat": "dev-master",
    "respect/validation": "0.9.*@dev",
    "slim/twig-view": "^2.0",
    "slim/flash": "^0.1.0",
    "hassankhan/config": "^0.9.1",
    "ergy/slim-annotations-router": "^1.0"
  },
  "autoload": {
    "classmap": [
      "src/App/Models/"
    ],
    "psr-4": {
      "": "src/",
      "PstStack\\": "src/",
      "App\\": "src/App"
    }
  }
}
